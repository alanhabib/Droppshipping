 { Link } from \"react-router-dom\";\n\nexport default class Modal extends Component {\n  render() {\n    return (\n      <ProductConsumer>\n        {value => {\n          const { modalOpen, closeModal } = value;\n          const { img, title, price } = value.modalProduct;\n          if (!modalOpen) {\n            return null;\n          } else {\n            return (\n              <ModalContainer>\n                <div className=\"container\">\n                  <div className=\"row\">\n                    <div id=\"modal\" className=\"column\">\n                      <h1>item added to the cart</h1>\n                      <img src={img} className=\"img-fluid\" alt=\"product\"></img>\n                      <h1>{title}</h1>\n                      <h5>price: {price}</h5>\n                      <Link to=\"/\">\n                        <ButtonContainer\n                          onClick={() => {\n                            closeModal();\n                          }}\n                        >\n                          store\n                        </ButtonContainer>\n                      </Link>\n                      <Link to=\"/cart\">\n                        <ButtonContainer cart onClick={() => closeModal()}>\n                          go to cart\n                        </ButtonContainer>\n                      </Link>\n                    </div>\n                  </div>\n                </div>\n              </ModalContainer>\n            );\n          }\n        }}\n      </ProductConsumer>\n    );\n  }\n}\n\nconst ModalContainer = styled.div`\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background: rgba(0, 0, 0, 0.3);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  #modal {\n    background: white;\n  }\n`;\n"},"lineCount":null}},"error":null,"hash":"e968eefb96ccf32d8df91923030a4c94","cacheData":{"env":{}}}